<div>
    <dl>
        <dt><%= Rental.human_attribute_name(:token) %></dt>
        <dd><%= rental.token %></dd>
        <dt><%= Rental.human_attribute_name(:start_date) %></dt>
        <dd><%= l rental.start_date %></dd>
        <dt><%= Rental.human_attribute_name(:end_date) %></dt>
        <dd><%= l rental.end_date %></dd>
        <dt><%= Rental.human_attribute_name(:car_category) %></dt>
        <dd><%= rental.car_category.name %></dd>
        <dt> Previs√£o de valor </dt>
        <dd><%= number_to_currency rental.total %></dd>
        <dt><%= Rental.human_attribute_name(:customer) %></dt>
        <dd><%= rental.customer.name %></dd>
        <dt><%= Customer.human_attribute_name(:cpf) %></dt>
        <dd><%= rental.customer.cpf %></dd>
        <dt><%= Customer.human_attribute_name(:email) %></dt>
        <dd><%= rental.customer.email %></dd>
        <dt><%= Rental.human_attribute_name(:user) %></dt>
        <dd><%= rental.user.name %></dd>
        <dt><%= User.human_attribute_name(:email) %></dt>
        <dd><%= rental.user.email %></dd>
    </dl>
    <%= link_to 'Ver detalhes', rental %>
</div>